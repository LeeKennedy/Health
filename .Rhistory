geom_point(shape = 21, fill = "cornflowerblue", colour = "blue", size = 4) +
geom_smooth(method=loess, colour = "darkgreen") +
labs(title = "Weight, January 2015 onwards", y="kg", x="") +
theme_bw() +
scale_y_continuous(breaks = seq(82, 98, by=1))
