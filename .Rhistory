ggplot(weight1, aes(Date,Weight)) +
geom_point(shape = 21, fill = "cornflowerblue", colour = "blue", size = 4) +
geom_smooth(colour = "darkgreen") +
ggtitle("Weight") +
ylim(83,105) +
theme_bw()
